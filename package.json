{
  "name": "furious-commander",
  "version": "1.0.1",
  "description": "CLI framework with decorators",
  "keywords": [
    "furious-commander",
    "cli",
    "cli-framework",
    "yargs",
    "nugaon"
  ],
  "homepage": "https://github.com/nugaon/furious-commander",
  "bugs": {
    "url": "https://github.com/nugaon/furious-commander/issues/"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/nugaon/furious-commander.git"
  },
  "scripts": {
    "compile": "webpack --progress --mode development",
    "compile:prod": "webpack --progress --mode production",
    "compile:types": "tsc --project tsconfig.d.json",
    "test": "jest --config=jest.config.ts",
    "lint": "eslint --fix \"src/**/*.ts\" \"test/**/*.ts\" && prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "check:lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\" && prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "check:types": "tsc --project tsconfig.test.json",
    "do-publish": "npm i && npm run test && npm run check:lint && npm run check:types && npm run compile:prod && npm run compile:types && npm login && npm publish"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.16",
    "@types/terser-webpack-plugin": "^5.0.2",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^5.11.0",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.3.0"
  },
  "dependencies": {
    "cafe-args": "^1.0.6",
    "reflect-metadata": "^0.1.13"
  }
}
